  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>HRDB</DataSourceName>
        <CommandText>SELECT  cn.ID		AS ContinentID		,
		cn.Name		AS ContinentName	,
		cu.ID		AS CountryID		,
		cu.Name		AS CountryName		,
		st.ID		AS StateID			,
		st.Code		AS StateCode		,
		st.Name     AS StateName		,
		c.ID		AS CityID			,
		c.Name		AS CityName	
FROM  dbo.Continent cn
LEFT JOIN dbo.Country cu
	ON cn.ID	=	cu.ContinentID
LEFT JOIN dbo.State st
	ON cu.ID	=	st.CountryID
LEFT JOIN dbo.City c
	ON c.StateID		=	st.ID </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ContinentID">
          <DataField>ContinentID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ContinentName">
