  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="EmpID_FullName">
          <DataField>EmpID_FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PaycheckAmount">
          <DataField>PaycheckAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>HRDB</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT	CONCAT(e.EmpNo, ' : ', e.FirstName, ' ', e.MiddleName, ' ', e.LastName) AS EmpID_FullName	,
		FORMAT(ep.FromDate, 'MMMM') AS Month	,
		ep.PaycheckAmount
FROM	dbo.Employee e
INNER JOIN EmployeePaycheck ep
	ON	e.ID	=	ep.EmpID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
