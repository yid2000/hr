        </Field>
        <Field Name="CityID">
          <DataField>CityID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CityName">
          <DataField>CityName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>HRDB</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT  cn.ID		AS ContinentID		,
		cn.Name		AS ContinentName	,
		cu.ID		AS CountryID		,
		cu.Name		AS CountryName		,
		st.ID		AS StateID			,
		st.Code		AS StateCode		,
		st.Name     AS StateName		,
		c.ID		AS CityID			,
		c.Name		AS CityName	
FROM  dbo.Continent cn
LEFT JOIN dbo.Country cu
	ON cn.ID	=	cu.ContinentID
LEFT JOIN dbo.State st
	ON cu.ID	=	st.CountryID
LEFT JOIN dbo.City c
	ON c.ID		=	st.ID 
ORDER BY cn.ID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
